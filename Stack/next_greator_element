vector<int>next_greator(vector<int>&arr)
{
    int n=arr.size();
    stack<int>s;
    vector<int>res(n,-1);
    for(int i=0;i<n;i++)
    {
        while(!s.empty() && arr[s.top()]<arr[i])
        {
            res[s.top()]=arr[i];
            s.pop();
        }
        s.push(i);
    }
return res;
  }
